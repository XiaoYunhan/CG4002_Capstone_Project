ACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"PastData.vue","sourceRoot":"src/components","sourcesContent":["<template>\r\n  <v-container>\r\n    <div style=\"width: 20%; float:left\">\r\n      <psnd />\r\n    </div>\r\n    <div style=\"width: 80%; float:right\">\r\n      <v-row>\r\n        <v-col class=\"mx-auto\" md=\"3\">\r\n          <v-select\r\n            v-model=\"selectedChart\"\r\n            :items=\"chartType\"\r\n            item-text=\"name\"\r\n            item-value=\"value\"\r\n            filled\r\n            label=\"Chart Type\"\r\n            background-color=\"#A9D66E\"\r\n            prepend-icon=\"mdi-view-dashboard\"\r\n            clearable\r\n            dense\r\n            color=\"#000000\"\r\n            @input=\"selectChart\"\r\n            @click:clear=\"clearCharts\"\r\n          />\r\n        </v-col>\r\n\r\n        <v-col class=\"mx-auto\" md=\"2\">\r\n          <v-text-field\r\n            v-model=\"search\"\r\n            append-icon=\"mdi-magnify\"\r\n            clearable\r\n            @click:clear=\"clearSearch\"\r\n            label=\"Search\"\r\n            single-line\r\n            color=\"#000000\"\r\n            @keydown.enter=\"searchData\"\r\n            @click:append=\"searchData\"\r\n          ></v-text-field>\r\n        </v-col>\r\n\r\n        <v-col class=\"mx-auto\" md=\"3\">\r\n          <v-select\r\n            v-model=\"selectedDancers\"\r\n            :items=\"dancers\"\r\n            item-text=\"name\"\r\n            item-value=\"value\"\r\n            filled\r\n            label=\"Dancer(s)\"\r\n            background-color=\"#00D19C\"\r\n            prepend-icon=\"mdi-account-multiple\"\r\n            clearable\r\n            multiple\r\n            dense\r\n            color=\"#000000\"\r\n            @input=\"selectDancers\"\r\n            @click:clear=\"clearDancers\"\r\n          />\r\n        </v-col>\r\n        <v-col class=\"mx-auto\" md=\"3\">\r\n          <v-menu\r\n            v-model=\"dateMenu\"\r\n            :nudge-right=\"40\"\r\n            :close-on-content-click=\"false\"\r\n            transition=\"scale-transition\"\r\n            offset-y\r\n            max-width=\"290px\"\r\n            min-width=\"290px\"\r\n          >\r\n            <template v-slot:activator=\"{ on }\">\r\n              <v-text-field\r\n                label=\"Choose Date(s)\"\r\n                :value=\"dateDisp\"\r\n                v-on=\"on\"\r\n                clearable\r\n                prepend-icon=\"mdi-calendar\"\r\n                background-color=\"#3DDBD8\"\r\n                filled\r\n                dense\r\n                color=\"#000000\"\r\n                @click:clear=\"clearDates\"\r\n              />\r\n            </template>\r\n            <v-date-picker\r\n              v-model=\"dateVal\"\r\n              @click:date=\"selectDates\"\r\n              multiple\r\n            />\r\n          </v-menu>\r\n        </v-col>\r\n        <v-col class=\"mx-auto\" md=\"1\">\r\n          <v-btn icon color=\"green\" fab @click=\"submit\">\r\n            <v-icon>mdi-check</v-icon>\r\n          </v-btn>\r\n        </v-col>\r\n      </v-row>\r\n      <v-list>\r\n        <v-card>\r\n          <v-card-title>\r\n            Dance Set\r\n            <v-spacer></v-spacer>\r\n            <v-text-field\r\n              v-model=\"tableSearch\"\r\n              append-icon=\"mdi-magnify\"\r\n              clearable\r\n              @click:clear=\"clearTableSearch\"\r\n              color=\"#000000\"\r\n              label=\"Search\"\r\n              single-line\r\n              hide-details\r\n              @keydown.enter=\"searchTable\"\r\n              @click:append=\"searchTable\"\r\n            ></v-text-field>\r\n          </v-card-title>\r\n          <!-- <v-data-table :headers=\"headers\" :items=\"dances\" :search=\"tableSearch\"></v-data-table> -->\r\n        </v-card>\r\n      </v-list>\r\n    </div>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\n// import axios from \"axios\";\r\nimport psnd from \"./PersistentSideNavDrawer\";\r\nimport Swal from \"sweetalert2\";\r\n\r\nexport default {\r\n  name: \"PastData\",\r\n\r\n  components: {\r\n    psnd,\r\n  },\r\n  data: () => ({\r\n    search: null,\r\n    tableSearch: null,\r\n    selectedChart: null,\r\n    chartType: [\r\n      { name: \"Dance Table\", value: \"Table\" },\r\n      { name: \"Sync Chart\", value: \"Pie\" },\r\n    ],\r\n    selectedDancers: null,\r\n    dancers: [\r\n      { name: \"Jingxuan\", value: \"Jingxuan\" },\r\n      { name: \"Karan\", value: \"Karan\" },\r\n      { name: \"Kexin\", value: \"Kexin\" },\r\n      { name: \"Sarah\", value: \"Sarah\" },\r\n      { name: \"Tristy\", value: \"Tristy\" },\r\n      { name: \"Yunhan\", value: \"Yunhan\" },\r\n    ],\r\n    dateMenu: false,\r\n    dateVal: null,\r\n  }),\r\n  computed: {\r\n    dateDisp() {\r\n      return this.dateVal;\r\n    },\r\n  },\r\n  methods: {\r\n    clearCharts: function() {\r\n      this.selectedChart = null;\r\n      this.selectChart();\r\n    },\r\n    selectChart: function() {\r\n      if (this.selectedChart != null) {\r\n        if (this.selectedChart === \"Table\") {\r\n          console.log(\"get data from Dance\");\r\n        } else if (this.selectedChart === \"Pie\") {\r\n          console.log(\"get data from Sync\");\r\n        }\r\n      } else {\r\n        console.log(\"get data from Dance and Sync\");\r\n      }\r\n    },\r\n    clearDancers: function() {\r\n      this.selectedDancers = null;\r\n      this.selectDancers();\r\n    },\r\n    selectDancers: function() {\r\n      if (this.selectedDancers != null) {\r\n        if (this.selectedDancers.length > 3) {\r\n          Swal.fire({\r\n            icon: \"error\",\r\n            title: \"Oops...\",\r\n            text: \"A maximum of 3 dancers can be selected!\",\r\n          });\r\n          this.selectedDancers.pop();\r\n        } else {\r\n          console.log(this.selectedDancers.length);\r\n          for (var i = 0; i < this.selectedDancers.length; i++) {\r\n            console.log(this.selectedDancers[i]);\r\n          }\r\n        }\r\n      } else {\r\n        console.log(\"display all dancers\");\r\n      }\r\n    },\r\n    clearDates: function() {\r\n      this.dateVal = null;\r\n      this.selectDates();\r\n    },\r\n    selectDates: function() {\r\n      if (this.dateVal != null) {\r\n        console.log(this.dateVal.length);\r\n        for (var j = 0; j < this.dateVal.length; j++) {\r\n          console.log(this.dateVal[j]);\r\n        }\r\n      } else {\r\n        console.log(\"display all dates\");\r\n      }\r\n    },\r\n    searchData: function() {\r\n      if (this.search != null) {\r\n        console.log(this.search);\r\n      } else {\r\n        console.log(\"search cleared\");\r\n      }\r\n    },\r\n    clearSearch: function() {\r\n      this.search = null;\r\n      this.searchData();\r\n    },\r\n    searchTable: function() {\r\n      if (this.tableSearch != null) {\r\n        console.log(this.tableSearch);\r\n      } else {\r\n        console.log(\"table search cleared\");\r\n      }\r\n    },\r\n    clearTableSearch: function() {\r\n      this.tableSearch = null;\r\n      this.searchTable();\r\n    },\r\n    submit: function() {\r\n      // console.log(\"Chart: \" + this.selectedChart);\r\n      // console.log(\"Dancers: \" + this.selectedDancers);\r\n      // console.log(\"Search Data: \" + this.search);\r\n      // console.log(\"Dates: \" + this.dateVal);\r\n\r\n      // data in selectedDancers are to be taken as AND i.e. the dance sets must contain the dancer names\r\n      // data in dateVal is to be taken as OR\r\n      // data in search is to be taken as OR\r\n      // selectedChart refers to the DB Table (Table: Dance, Pie: Sync)\r\n      // SELECT * FROM Dance/Sync WHERE (this.selectedDancers) AND (this.dateVal) AND (this.search)\r\n      const pastDataToSend =\r\n        this.selectedChart +\r\n        \"|\" +\r\n        this.selectedDancers +\r\n        \"|\" +\r\n        this.search +\r\n        \"|\" +\r\n        this.dateVal;\r\n      console.log(\"Past Data To Send: \" + pastDataToSend);\r\n    },\r\n  },\r\n  // data() {\r\n  //   return {\r\n  //     search: \"\",\r\n  //     headers: [\r\n  //       {\r\n  //         text: \"Date\",\r\n  //         align: \"start\",\r\n  //         sortable: true,\r\n  //         value: \"date\",\r\n  //       },\r\n  //       { text: \"Dance Position\", value: \"dance_position\" },\r\n  //       { text: \"Left Timestamp\", value: \"left_timestamp\" },\r\n  //       { text: \"Left Position\", value: \"left_position\" },\r\n  //       { text: \"Center Timestamp\", value: \"center_timestamp\" },\r\n  //       { text: \"Center Position\", value: \"center_position\" },\r\n  //       { text: \"Right Timestamp\", value: \"right_timestamp\" },\r\n  //       { text: \"Right Position\", value: \"right_position\" },\r\n  //       {\r\n  //         text: \"Difference between fastest and slowest dancer (s)\",\r\n  //         value: \"diff\",\r\n  //       },\r\n  //       { text: \"Sync\", value: \"sync\" },\r\n  //     ],\r\n  //     dances: [\r\n  //       {\r\n  //         date: \"24/08/2020\",\r\n  //         dance_position: \"Start\",\r\n  //         left_timestamp: \"10:44:00\",\r\n  //         left_position: \"Jingxuan\",\r\n  //         center_timestamp: \"10:44:00\",\r\n  //         center_position: \"Kexin\",\r\n  //         right_timestamp: \"10:44:00\",\r\n  //         right_position: \"Karan\",\r\n  //         diff: 0,\r\n  //         sync: \"Yes\",\r\n  //       },\r\n  //       {\r\n  //         date: \"24/08/2020\",\r\n  //         dance_position: \"Rocket\",\r\n  //         left_timestamp: \"10:44:05\",\r\n  //         left_position: \"Kexin\",\r\n  //         center_timestamp: \"10:44:08\",\r\n  //         center_position: \"Karan\",\r\n  //         right_timestamp: \"10:44:07\",\r\n  //         right_position: \"Jingxuan\",\r\n  //         diff: 3,\r\n  //         sync: \"No\",\r\n  //       },\r\n  //       {\r\n  //         date: \"24/08/2020\",\r\n  //         dance_position: \"End\",\r\n  //         left_timestamp: \"10:45:00\",\r\n  //         left_position: \"Karan\",\r\n  //         center_timestamp: \"10:45:00\",\r\n  //         center_position: \"Kexin\",\r\n  //         right_timestamp: \"10:45:03\",\r\n  //         right_position: \"Jingxuan\",\r\n  //         diff: 3,\r\n  //         sync: \"No\",\r\n  //       },\r\n  //     ],\r\n  //   };\r\n  // },\r\n  //   data: () => ({\r\n  //     date: \"\",\r\n  //   }),\r\n  //   beforeMount() {\r\n  //     this.getUserdate();\r\n  //   },\r\n  //   methods: {\r\n  //     getUserdate: function() {\r\n  //       axios.get(\"/api/date\").then((response) => {\r\n  //         console.log(response.data);\r\n  //         this.date = response.data;\r\n  //       });\r\n  //     },\r\n  //   },\r\n};\r\n</script>\r\n"]}]}