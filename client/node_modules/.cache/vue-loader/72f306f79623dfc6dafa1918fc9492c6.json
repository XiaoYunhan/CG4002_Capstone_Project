;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"RealTimeDashboard.vue","sourceRoot":"src/components","sourcesContent":["<template>\r\n  <v-container>\r\n    <div style=\"width: 20%; float:left\">\r\n      <psnd />\r\n    </div>\r\n    <div style=\"width: 80%; float:right\">\r\n      <v-row>\r\n        <v-col class=\"mx-auto\" md=\"3\">\r\n          <v-select\r\n            v-model=\"selectDancer_1\"\r\n            :items=\"dancer_1\"\r\n            item-text=\"name\"\r\n            item-value=\"value\"\r\n            filled\r\n            label=\"Dancer 1\"\r\n            background-color=\"#A9D66E\"\r\n            prepend-icon=\"mdi-account\"\r\n            dense\r\n            color=\"#000000\"\r\n            @input=\"selectDancer1\"\r\n          ></v-select>\r\n        </v-col>\r\n        <v-spacer />\r\n        <v-col class=\"mx-auto\" md=\"3\">\r\n          <v-select\r\n            v-model=\"selectDancer_2\"\r\n            :items=\"dancer_2\"\r\n            item-text=\"name\"\r\n            item-value=\"value\"\r\n            filled\r\n            label=\"Dancer 2\"\r\n            background-color=\"#00D19C\"\r\n            prepend-icon=\"mdi-account\"\r\n            dense\r\n            color=\"#000000\"\r\n            @input=\"selectDancer2\"\r\n          ></v-select>\r\n        </v-col>\r\n        <v-spacer />\r\n        <v-col class=\"mx-auto\" md=\"3\">\r\n          <v-select\r\n            v-model=\"selectDancer_3\"\r\n            :items=\"dancer_3\"\r\n            item-text=\"name\"\r\n            item-value=\"value\"\r\n            filled\r\n            label=\"Dancer 3\"\r\n            background-color=\"#3DDBD8\"\r\n            prepend-icon=\"mdi-account\"\r\n            dense\r\n            color=\"#000000\"\r\n            @input=\"selectDancer3\"\r\n          ></v-select>\r\n        </v-col>\r\n        <v-col class=\"mx-auto\" md=\"1\">\r\n          <v-btn icon color=\"green\" fab @click=\"submit\">\r\n            <v-icon>mdi-check</v-icon>\r\n          </v-btn>\r\n        </v-col>\r\n      </v-row>\r\n      <template v-if=\"nodata\">\r\n        <v-card class=\"mx-auto\" width=\"60%\" height=\"160\" color=\"#ECEFF1\" rounded>\r\n          <v-card-title>\r\n            <v-row align=\"center\" justify=\"center\">\r\n              <v-icon light large center>mdi-timer-sand</v-icon>\r\n            </v-row>\r\n          </v-card-title>\r\n          <p class=\"text-center\">\r\n            There is no live dance class happening at the moment.\r\n            <br />Please wait for a session to start.\r\n            <br />Thank you!\r\n          </p>\r\n        </v-card>\r\n      </template>\r\n      <template v-else>\r\n        <h1 style=\"text-align:center;font-size:50px;font-weight:bold;\">Predicted Dance Move</h1>\r\n        <h1 style=\"text-align:center;font-size:50px;font-weight:bold;\">Dancer Positions</h1>\r\n        <br />\r\n        <v-row class=\"mx-auto\">\r\n          <v-card width=\"50%\" height=\"360\" class=\"mx-auto\">\r\n            <v-card-title>\r\n              Dance Set\r\n              <v-spacer />\r\n              <v-text-field\r\n                v-model=\"tableSearch\"\r\n                append-icon=\"mdi-magnify\"\r\n                clearable\r\n                @click:clear=\"clearTableSearch\"\r\n                color=\"#000000\"\r\n                label=\"Search\"\r\n                single-line\r\n                hide-details\r\n                @keydown.enter=\"searchTable\"\r\n                @click:append=\"searchTable\"\r\n              ></v-text-field>\r\n            </v-card-title>\r\n            <v-data-table :headers=\"headers\" :items=\"dances\" :search=\"tableSearch\"></v-data-table>\r\n            <!-- <h1 style=\"text-align:center;\">Data Table</h1> -->\r\n          </v-card>\r\n          <v-card width=\"30%\" height=\"360\" class=\"mx-auto\">\r\n            <v-card-title>Sync Dance Set</v-card-title>\r\n            <PieChart />\r\n            <!-- <h1 style=\"text-align:center;\">Sync Pie Chart</h1> -->\r\n          </v-card>\r\n        </v-row>\r\n      </template>\r\n    </div>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport psnd from \"./PersistentSideNavDrawer\";\r\nimport Swal from \"sweetalert2\";\r\nimport PieChart from \"./Charts/PieChart\";\r\n\r\nexport default {\r\n  name: \"RealTimeDashboard\",\r\n\r\n  components: {\r\n    psnd,\r\n    PieChart,\r\n  },\r\n  data: () => ({\r\n    headers: [\r\n      { text: \"Dance Position\", value: \"dance_position\" },\r\n      { text: \"Left Position\", value: \"left_position\" },\r\n      { text: \"Center Position\", value: \"center_position\" },\r\n      { text: \"Right Position\", value: \"right_position\" },\r\n      {\r\n        text: \"Difference in timing (s)\",\r\n        value: \"diff\",\r\n      },\r\n    ],\r\n    dances: [\r\n      {\r\n        dance_position: \"Start\",\r\n        left_position: \"Jingxuan\",\r\n        center_position: \"Kexin\",\r\n        right_position: \"Karan\",\r\n        diff: 0,\r\n      },\r\n      {\r\n        dance_position: \"Rocket\",\r\n        left_position: \"Kexin\",\r\n        center_position: \"Karan\",\r\n        right_position: \"Jingxuan\",\r\n        diff: 3,\r\n      },\r\n      {\r\n        dance_position: \"End\",\r\n        left_position: \"Karan\",\r\n        center_position: \"Kexin\",\r\n        right_position: \"Jingxuan\",\r\n        diff: 3,\r\n      },\r\n    ],\r\n    tableSearch: null,\r\n    selectDancer_1: null,\r\n    selectDancer_2: null,\r\n    selectDancer_3: null,\r\n    dancer_1: [\r\n      { name: \"Jingxuan\", value: \"Jingxuan\" },\r\n      { name: \"Karan\", value: \"Karan\" },\r\n      { name: \"Kexin\", value: \"Kexin\" },\r\n      { name: \"Sarah\", value: \"Sarah\" },\r\n      { name: \"Tristy\", value: \"Tristy\" },\r\n      { name: \"Yunhan\", value: \"Yunhan\" },\r\n    ],\r\n    dancer_2: [\r\n      { name: \"Jingxuan\", value: \"Jingxuan\" },\r\n      { name: \"Karan\", value: \"Karan\" },\r\n      { name: \"Kexin\", value: \"Kexin\" },\r\n      { name: \"Sarah\", value: \"Sarah\" },\r\n      { name: \"Tristy\", value: \"Tristy\" },\r\n      { name: \"Yunhan\", value: \"Yunhan\" },\r\n    ],\r\n    dancer_3: [\r\n      { name: \"Jingxuan\", value: \"Jingxuan\" },\r\n      { name: \"Karan\", value: \"Karan\" },\r\n      { name: \"Kexin\", value: \"Kexin\" },\r\n      { name: \"Sarah\", value: \"Sarah\" },\r\n      { name: \"Tristy\", value: \"Tristy\" },\r\n      { name: \"Yunhan\", value: \"Yunhan\" },\r\n    ],\r\n    nodata: true,\r\n  }),\r\n  methods: {\r\n    searchTable: function () {\r\n      if (this.tableSearch != null) {\r\n        console.log(this.tableSearch);\r\n      } else {\r\n        console.log(\"table search cleared\");\r\n      }\r\n    },\r\n    clearTableSearch: function () {\r\n      this.tableSearch = null;\r\n      this.searchTable();\r\n    },\r\n    selectDancer1: function () {\r\n      console.log(this.selectDancer_1);\r\n    },\r\n    selectDancer2: function () {\r\n      console.log(this.selectDancer_2);\r\n    },\r\n    selectDancer3: function () {\r\n      console.log(this.selectDancer_3);\r\n    },\r\n    // comment out for first and second evaluation after Individual test\r\n    checkDancers: function () {\r\n      if (this.selectDancer_1 == null) {\r\n        Swal.fire({\r\n          icon: \"error\",\r\n          title: \"Oops...\",\r\n          text: \"Please select a Dancer 1\",\r\n        });\r\n      } else if (this.selectDancer_2 == null) {\r\n        Swal.fire({\r\n          icon: \"error\",\r\n          title: \"Oops...\",\r\n          text: \"Please select a Dancer 2\",\r\n        });\r\n      } else if (this.selectDancer_3 == null) {\r\n        Swal.fire({\r\n          icon: \"error\",\r\n          title: \"Oops...\",\r\n          text: \"Please select a Dancer 3\",\r\n        });\r\n      } else if (\r\n        this.selectDancer_1 != null &&\r\n        this.selectDancer_2 != null &&\r\n        this.selectDancer_3 != null\r\n      ) {\r\n        if (\r\n          this.selectDancer_1 == this.selectDancer_2 &&\r\n          this.selectDancer_2 == this.selectDancer_3 &&\r\n          this.selectDancer_1 == this.selectDancer_3\r\n        ) {\r\n          Swal.fire({\r\n            icon: \"error\",\r\n            title: \"Oops...\",\r\n            text: \"Dancers 1, 2 and 3 cannot be the same!\",\r\n          });\r\n          this.selectDancer_2 = null;\r\n          this.selectDancer_3 = null;\r\n        } else if (this.selectDancer_1 == this.selectDancer_2) {\r\n          Swal.fire({\r\n            icon: \"error\",\r\n            title: \"Oops...\",\r\n            text: \"Dancers 1 and 2 cannot be the same!\",\r\n          });\r\n          this.selectDancer_2 = null;\r\n        } else if (this.selectDancer_1 == this.selectDancer_3) {\r\n          Swal.fire({\r\n            icon: \"error\",\r\n            title: \"Oops...\",\r\n            text: \"Dancers 1 and 3 cannot be the same!\",\r\n          });\r\n          this.selectDancer_3 = null;\r\n        } else if (this.selectDancer_2 == this.selectDancer_3) {\r\n          Swal.fire({\r\n            icon: \"error\",\r\n            title: \"Oops...\",\r\n            text: \"Dancers 2 and 3 cannot be the same!\",\r\n          });\r\n          this.selectDancer_3 = null;\r\n        }\r\n      }\r\n    },\r\n    submit: function () {\r\n      this.checkDancers();\r\n      const jsonDancers =\r\n        '{ \"dancer1\":\"' +\r\n        this.selectDancer_1 +\r\n        '\", \"dancer2\":\"' +\r\n        this.selectDancer_2 +\r\n        '\", \"dancer3\":\"' +\r\n        this.selectDancer_3 +\r\n        '\" }';\r\n      var dancers = JSON.parse(jsonDancers);\r\n      console.log(\"JSON Dancers: \" + JSON.stringify(dancers));\r\n\r\n      axios\r\n        .post(\"/dashboard/real-time-dashboard\", {\r\n          realTimeDashboard: dancers,\r\n        })\r\n        .then((response) => {\r\n          console.log(response.data);\r\n        });\r\n    },\r\n  },\r\n  //   data: () => ({\r\n  //     name: \"\",\r\n  //   }),\r\n  //   beforeMount() {\r\n  //     this.getUserName();\r\n  //   },\r\n  //   methods: {\r\n  //     getUserName: function() {\r\n  //       axios.get(\"/api/name\").then((response) => {\r\n  //         console.log(response.data);\r\n  //         this.name = response.data;\r\n  //       });\r\n  //     },\r\n  //   },\r\n};\r\n</script>\r\n"]}]}